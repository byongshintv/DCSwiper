{
    "manifest_version": 2,
    "name": "DCSwiper",
    "version": "0.2",
    "description": "중갤 돌갤 완장용 청소기",
    "content_scripts": [
        {
            "matches": ["http://gall.dcinside.com/*"],
            "js": [
                "src/jquery.min.js","src/main.bundle.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icons/size128.png",
        "default_popup": "popup.html"
    }, 
    "icons": { 
        "16": "icons/size16.png",
        "48": "icons/size48.png",
        "128": "icons/size128.png" 
    },
    "permissions": [
        "activeTab",
        "storage",
        "http://gall.dcinside.com/*"
    ]
}